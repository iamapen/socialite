{
    "name": "iamapen/socialite",
    "description": "Forked /laravel/socialite for Laravel 5.5 LTS.",
    "keywords": ["oauth", "laravel"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/iamapen/socialite/issues",
        "source": "https://github.com/iamapen/socialite"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/contracts": "~5.5",
        "illuminate/http": "~5.5",
        "illuminate/support": "~5.5",
        "league/oauth1-client": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Socialite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Socialite\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
        },
        "laravel": {
            "providers": [
                "Laravel\\Socialite\\SocialiteServiceProvider"
            ],
            "aliases": {
                "Socialite": "Laravel\\Socialite\\Facades\\Socialite"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
